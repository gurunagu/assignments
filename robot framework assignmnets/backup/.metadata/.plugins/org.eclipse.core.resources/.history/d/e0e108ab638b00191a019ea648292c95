*** Keywords ***
print data
    [Arguments]    ${message}
    log    ${message}
    Log To Console    ${message}       
    
positional keywords
    [Arguments]    ${brand}    ${phone_model}    ${os}
    log to console    \n
    print data    brand:${brand}
    print data    phone model:${phone_model}
    print data    os:${os}
    
default keywords
    [Arguments]    ${brand}    ${phone_model}    ${os}=android
    print data    brand:${brand}
    print data    phone model:${phone_model}
    print data    os:${os}
    
named keywords
    [Arguments]    ${brand}    ${phone_model}    ${os}=android
    print data    brand:${brand}
    print data    phone model:${phone_model}
    print data    os:${os}
    